<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="io.github.tonyshkurenko.coroutinesexample">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:name=".App"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".examples.LaunchActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>

        <activity android:name=".examples.ActorActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>

        <activity android:name=".examples.AsyncActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>

        <activity android:name=".examples.ProduceActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>

        <activity android:name=".examples.SelectActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>

        <activity android:name=".examples.YieldActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>
        <activity android:name=".examples.CrScopeActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>
        <activity android:name=".examples.ChannelsActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>
        <activity android:name=".examples.RealLifeActivity"
                  android:parentActivityName=".MainActivity"
        >
        </activity>


    </application>

</manifest>